{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.json",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "^/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "^/[^/]+\\.(js|css|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot|json)$",
      "dest": "/$0"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": ".",
  "cleanUrls": true
}
